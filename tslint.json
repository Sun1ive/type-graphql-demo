{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": false,
    "trailing-comma": false,
    "member-access": false,
    "no-console": false,
    "no-var-requires": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "ordered-imports": false,
    "max-line-length": true
  },
  "linterOptions": {
    "exclude": ["node_modules/*"]
  }
}
